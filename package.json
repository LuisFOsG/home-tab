{
  "name": "home-tab",
  "version": "0.11.2",
  "scripts": {
    "co": "npm run lint && git add . && sui-mono commit",
    "deploy": "npm run build && surge dist --domain https://hometab.surge.sh",
    "lint": "standard --fix",
    "dev": "vite",
    "build": "vite build",
    "build:background": "cross-env VITE_EXTENSION=true vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@s-ui/mono": "2.18.0",
    "autoprefixer": "10.4.2",
    "postcss": "8.4.5",
    "pug": "3.0.2",
    "rollup-plugin-copy": "3.4.0",
    "standard": "16.0.4",
    "tailwindcss": "3.0.15",
    "vite": "2.7.13",
    "vite-plugin-chrome-extension": "0.0.7",
    "vite-plugin-html": "2.1.2"
  },
  "dependencies": {
    "cross-env": "7.0.3"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/standard/eslintrc.json"
    ]
  },
  "standard": {
    "ignore": [
      "/assets/",
      "/dist/"
    ]
  }
}
