    "lint": "standard --fix",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@s-ui/mono": "2.29.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@vitejs/plugin-react": "1.3.2",
    "autoprefixer": "10.4.7",
    "postcss": "8.4.14",
    "standard": "17.0.0",
    "tailwindcss": "3.1.3",
    "vite": "2.9.12",
    "vite-plugin-html": "3.2.0"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/standard/eslintrc.json"
    ]
  },
  "standard": {
    "ignore": [
      "/assets/",
      "/dist/"
    ]
  }
